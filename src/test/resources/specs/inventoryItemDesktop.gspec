@objects
    inventory_item_img              css img.inventory_item_img
    inventory_item_name             css div.inventory_item_name
    inventory_item_desc             css div.inventory_item_desc
    inventory_item_price            css div.inventory_item_price

= Item section =
    inventory_item_img:
        inside      parent
        width       143px
        height      250 to 300 px
        left-of     inventory_item_name 19px

    inventory_item_name:
        inside      parent
        width       285px
        height      20 to 50 px
        right-of    inventory_item_img 11 to 20 px
        above       inventory_item_desc 0 to 10 px